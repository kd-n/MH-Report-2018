<Serializable()> Public Class Team
    Private teamName As String
    Private teamMembers As List(Of String)

    Public Sub New(teamName As String, teamMembers As List(Of String))
        Me.teamName = teamName
        Me.teamMembers = teamMembers
    End Sub

    Public ReadOnly Property Name As String
        Get
            Return Me.teamName
        End Get
    End Property

    Public ReadOnly Property Members As List(Of String)
        Get
            Return Me.teamMembers
        End Get
    End Property
End Class
